pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                bat 'npm install'
            }
        }
        stage('parallel') {
            parallel {

                stage('Run tests in parallel A') {
                    steps {
                        bat 'npx cypress run --record --key fe0b4d63-745e-4f85-81b7-d52a653c0442 --parallel'
                    }
                }
                stage('Run tests in parallel B') {
                    steps {
                        bat 'npx cypress run --record --key fe0b4d63-745e-4f85-81b7-d52a653c0442 --parallel'
                    }
                }
                }
            }
        }
    }
}